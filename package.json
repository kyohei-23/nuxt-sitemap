    "radix3": "^1.1.2",
    "semver": "^7.7.1",
    "sirv": "^3.0.1",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.17.4",
    "@nuxt/content": "^3.4.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxt/ui": "^3.0.2",
    "@nuxtjs/i18n": "9.5.2",
    "@nuxtjs/robots": "^5.2.8",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-n": "^17.17.0",
    "execa": "^9.5.2",
    "nuxt": "^3.16.2",
    "nuxt-i18n-micro": "^1.83.1",
    "typescript": "5.8.3",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "resolutions": {
    "postcss": "8.5.3",
    "typescript": "5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "vue-demi"
    ]
  }
